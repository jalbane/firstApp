{
  "expo": {
    "name": "App",
    "slug": "firstApp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.user1.react",
      "supportsTablet": true
    },
    "android": {
      "package": "com.user1.react"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "Game Instructions: Flip all the values you see to display '1' to win. If you get stuck you can reset the board and try again by tapping the buttons below."
  }
